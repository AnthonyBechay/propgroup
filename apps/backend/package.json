{
  "name": "propgroup-backend",
  "version": "1.0.0",
  "description": "PropGroup Backend API",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "build": "echo 'No build step needed'",
    "db:generate": "cd ../packages/db && npm run db:generate",
    "db:push": "cd ../packages/db && npm run db:push",
    "db:migrate": "cd ../packages/db && npm run db:migrate",
    "db:seed": "node src/seed.js",
    "db:studio": "cd ../packages/db && npm run db:studio"
  },
  "keywords": ["real-estate", "api", "backend"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "zod": "^3.22.4",
    "@propgroup/db": "workspace:*"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}