{
  "name": "propgroup",
  "version": "1.0.0",
  "description": "PropGroup - A comprehensive real estate investment platform",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "setup:wizard": "node scripts/setup.js",
    "health": "node scripts/health-check.js",
    "migration:new": "node scripts/create-migration.js",
    "types:generate": "node scripts/generate-types.js",
    "dev": "npm run supabase:start && npm run --prefix apps/web dev",
    "build": "npm run build:packages && npm run --prefix apps/web build",
    "start": "npm run --prefix apps/web start",
    "clean": "npm run clean:all",
    "clean:all": "node scripts/clean.js",
    "clean:install": "npm run clean && npm install",
    "lint": "npm run --prefix apps/web lint",
    "lint:fix": "npm run --prefix apps/web lint:fix",
    "type-check": "npm run type-check:all",
    "type-check:all": "npm run --prefix packages/supabase type-check && npm run --prefix apps/web type-check",
    "test": "npm run test:all",
    "test:all": "npm run --prefix apps/web test:run",
    "test:watch": "npm run --prefix apps/web test",
    "test:coverage": "npm run --prefix apps/web test:coverage",
    "setup": "npm install && npm run supabase:setup",
    "setup:dev": "npm run setup && npm run dev",
    "build:packages": "npm run --prefix packages/supabase build && npm run --prefix packages/ui build && npm run --prefix packages/config build",
    "build:web": "npm run build:packages && npm run --prefix apps/web build",
    "build:mobile": "npm run build:packages && npm run --prefix apps/mobile-capacitor build",
    "deploy": "npm run build && npm run supabase:deploy",
    "deploy:web": "vercel --prod",
    "deploy:preview": "vercel",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,sql}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,sql}\"",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:setup": "supabase db push && supabase db seed",
    "supabase:reset": "supabase db reset",
    "supabase:migrate": "supabase migration new",
    "supabase:push": "supabase db push",
    "supabase:pull": "supabase db pull",
    "supabase:seed": "supabase db seed",
    "db:seed": "node packages/db/seed.js",
    "db:migrate": "node packages/db/migrate.js",
    "db:check": "node packages/db/check-database.js",
    "db:fix": "fix-all-database-issues.bat",
    "supabase:types": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > packages/supabase/src/types/database.ts",
    "supabase:functions:serve": "supabase functions serve",
    "supabase:functions:deploy": "supabase functions deploy",
    "supabase:deploy": "npm run supabase:push && npm run supabase:functions:deploy",
    "supabase:link": "supabase link",
    "supabase:logs": "supabase functions logs",
    "mobile:ios": "npm run --prefix apps/mobile-capacitor cap:ios",
    "mobile:android": "npm run --prefix apps/mobile-capacitor cap:android",
    "mobile:sync": "npm run --prefix apps/mobile-capacitor cap:sync"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "supabase": "^1.142.2",
    "typescript": "^5.0.0",
    "vercel": "^32.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "packageManager": "npm@9.0.0"
}
