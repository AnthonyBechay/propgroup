{
  "name": "@propgroup/db",
  "version": "1.0.0",
  "description": "Database schema and Prisma client",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "postinstall": "prisma generate",
    "build": "prisma generate && tsc",
    "dev": "tsc --watch",
